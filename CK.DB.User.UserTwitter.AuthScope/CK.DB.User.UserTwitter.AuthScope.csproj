    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <Description>This package adds a ScopeSetId column to CK.tUserTwitter table.</Description>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="CK.DB.Auth.AuthScope" Version="$(CKDBVersion)" />
